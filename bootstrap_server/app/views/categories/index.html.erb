<script type="text/javascript">
function messageParent(url)
{
	window.parent.postMessage(url, '*');
}
</script>
<table>
<% @categories.each do |category| %>
  <tr>
    <td><a href='#' onclick="messageParent('<%= category.id %>')"><%= category.name %></a> </td>
  </tr>
<% end %>
</table>
