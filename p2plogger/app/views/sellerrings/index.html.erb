<h1>Listing sellerrings</h1>

<table>
  <tr>
    <th>Node IP</th>
    <th>Predecessor IP</th>
    <th>Successor IP</th>
    <th>Category</th>
  </tr>

<% @sellerrings.each do |sellerring| %>
  <tr>
    <td><%= sellerring.ip %></td>
    <td><%= sellerring.predecessor %></td>
    <td><%= sellerring.successor %></td>
    <td><%= sellerring.category %></td>
  </tr>
<% end %>
</table>

