<h1>Listing users</h1>

<table>
  <tr>
    <th>Uid</th>
    <th>Uname</th>
    <th>Password</th>
    <th>Seller</th>
    <th>Keyurl</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.uid %></td>
    <td><%= user.uname %></td>
    <td><%= user.password %></td>
    <td><%= user.seller %></td>
    <td><%= user.keyurl %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
